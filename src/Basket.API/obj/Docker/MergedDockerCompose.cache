services:
  basket.api:
    build:
      context: C:\microservi√æos\src\catalog\Basket.API
      dockerfile: Dockerfile
    container_name: basket.api
    depends_on:
      basketdb:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      CacheSettings:ConnectionString: basketdb:6379
    image: basketapi
    ports:
    - published: 8001
      target: 80
  basketdb:
    container_name: basketdb
    image: redis
    ports:
    - published: 6379
      target: 6379
    restart: always
version: '3.4'